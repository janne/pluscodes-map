{
  "expo": {
    "name": "pluscodes-map",
    "description": "A very interesting project.",
    "slug": "pluscodes-map",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "your.package",
      "config": {
        "googleMaps": {
          "apiKey": "your-api-key"
        }
      }
    }
  }
}
